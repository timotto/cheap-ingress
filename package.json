    "jasmine-ts": "^0.2.1",
    "nodemon": "^1.12.1",
    "nyc": "^11.4.1",
    "typemoq": "^2.0.1",
    "typescript": "^2.5.2",
    "typescript-eslint-parser": "^13.0.0",
    "yarn": "^1.1.0"
  },
  "dependencies": {
    "dnsd": "^0.9.6",
    "express": "^4.16.2",
